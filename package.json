{
  "name": "goose-multi-interface",
  "version": "1.0.0",
  "description": "Multi-interface chat application with Goose CLI integration",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start-with-scheduler.js index.js",
    "start:no-scheduler": "node index.js",
    "cli": "node cli.js",
    "web": "node scripts/start-with-scheduler.js server.js",
    "web:no-scheduler": "node server.js",
    "scheduler": "node scheduler/schedule.js",
    "logs": "node log-observer.js",
    "raw-logs": "node raw-log-viewer.js",
    "clearall": "node scripts/clearall.js",
    "worktree:switch": "./scripts/worktree-switch.sh",
    "worktree:create": "./scripts/worktree-create.sh",
    "menu": "./menu.sh",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "test": "jest",
    "migrate-recipes": "node scripts/migrate-to-centralized.js",
    "migrate-recipes:dry-run": "node scripts/migrate-to-centralized.js --dry-run",
    "recipe-status": "node scripts/recipe-status.js",
    "setup": "node scripts/setup.js",
    "setup:force": "node scripts/setup.js --force",
    "setup:check": "node scripts/setup.js --check"
  },
  "dependencies": {
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.11.0",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "node-pty": "^1.0.0",
    "readline": "^1.3.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.6"
  },
  "keywords": [
    "goose",
    "cli",
    "chat",
    "ai",
    "multi-interface"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0"
  }
}
